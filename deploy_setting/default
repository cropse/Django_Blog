upstream django {
    server unix:///var/www/demo.sock;
}

server {
    listen 80;
    server_name 52.68.0.97;
    charset utf-8;
    client_max_body_size 20M;

    location /static {
          alias /home/ubuntu/blog/static_cdn;
    }

    location / {
        uwsgi_pass django;
        # uwsgi_pass 127.0.0.1:8080;
        include /etc/nginx/uwsgi_params;
    }



}
